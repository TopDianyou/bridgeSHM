QMainWindow
{

    border-image:url(./images/llhBackground.jpg);
}

QStackedWidget#mainStackedWidget
{
    background-color: rgba(25,50,100,80%);

}

QPushButton#realTimeBtn,#dataPreProcBtn,#consDiagBtn,#warnInfBtn,#userManBtn,#printRepBtn,#exitBtn
{
    border-image:url(./images/btnblue.png);
    color: rgb(214, 213, 213);
}

QPushButton#realTimeBtn:hover,#dataPreProcBtn:hover,#consDiagBtn:hover,#warnInfBtn:hover,#userManBtn:hover,#printRepBtn:hover,#exitBtn:hover
{
    border-image:url(./images/btnlightpurple.png);
}

QPushButton#realTimeBtn:pressed,#dataPreProcBtn:pressed,#consDiagBtn:pressed,#warnInfBtn:pressed,#userManBtn:pressed,#printRepBtn:pressed,#exitBtn:pressed
{
    border-image:url(./images/btnpurple.png);
    
}

QPushButton#threeDimBtn,#sensorDataBtn,#historyDataBtn
{
    border-image:url(./images/btntech1.png);
    color: rgb(214, 213, 213);
}
QPushButton#threeDimBtn:hover,#sensorDataBtn:hover,#historyDataBtn:hover
{
    border-image:url(./images/btntech2.png);
    color: rgb(214, 213, 213);
}

QLabel
{
    color:rgb(214, 213, 213);
}

QGroupBox
{
    color:rgb(214, 213, 213);
}

