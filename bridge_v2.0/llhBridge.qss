QTabWidget::pane
{
    /* background-color: rgba(25,50,100,90%); */
    border: 1px solid rgb(255,255,255);
}
QTabBar::tab
{
    color:white;
    background-color: rgba(54, 80, 167, 0.9);
    border: 1px solid rgb(255,255,255);
}

QTabBar::tab:selected, QTabBar::tab:hover 
{
    color:white;
    background-color: rgba(0, 140, 255, 0.8);
    border: 1px solid rgb(255,255,255);
}

QGroupBox#blockSelectGroupBox
{
    background-image:url(./images/bridgeView.png);
    background-repeat:no-repeat; 
    background-position:center; 
    
}

